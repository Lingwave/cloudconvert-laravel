{
    "name": "lingwave/cloudconvert-laravel",
    "description": "A Laravel wrapper for the CloudConvert API",
    "keywords": ["cloudconvert", "laravel", "laravel8", "screenshot", "conversion", "office", "documents"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robbie Paul",
            "email": "robbiepaul@me.com"
        },
        {
            "name": "Lingwave",
            "email": "dev@lingwave.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^8.0",
        "illuminate/filesystem": "^8.0",
        "guzzlehttp/guzzle": "~7.0",
        "patchwork/utf8": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.27",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "Lingwave\\CloudConvertLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lingwave\\CloudConvertLaravel\\CloudConvertLaravelServiceProvider"
            ],
            "aliases": {
                "CloudConvert": "Lingwave\\CloudConvertLaravel\\Facades\\CloudConvert"
            }
        }
    },
    "minimum-stability": "dev"
}
